E -> T E'
E' -> OPER_SUM T E' 
E' -> OPER_RESTA T E' 
E' -> SIG_MAYOR T E' 
E' -> SIG_MENOR T E' 
E' -> SIG_MAYORIGUAL T E' 
E' ->  SIG_MENORIGUAL T E' 
E' ->  SIG_DESPLA_IZ T E' 
E' ->  SIG_DESPLA_DE T E' 
E' -> OPER_DESPLA_IZ T E'
E' -> ''
T -> F T'
T' -> OPER_MUL F T' 
T' ->  OPER_DIV F T' 
T' ->  OPER_RESTO F T' 
T' ->  SIG_IGUAL F T' 
T' -> ''
F -> PARENT_INICIO E PARENT_FIN 
F -> LL_INICIO E LL_FIN 
F -> COMILLA_DOBLE E COMILLA_DOBLE 
F -> ID 
F -> NUM

BLOQUE -> LL_INICIO E LL_FIN

IF -> "IF" PARENT_INICIO E PARENT_FIN BLOQUE
IF -> "IF" PARENT_INICIO E PARENT_FIN BLOQUE "ELSE" BLOQUE

WHILE -> "TIPO_WHILE" PARENT_INICIO E PARENT_FIN BLOQUE

DO -> "TIPO_DO-WHILE" BLOQUE "WHILE" PARENT_INICIO E PARENT_FIN PUNT_COMA

FOR -> "TIPO_FOR" PARENT_INICIO ASIGNACION PUNT_COMA E PUNT_COMA ASIGNACION PARENT_FIN BLOQUE

ASIGNACION -> ID SIG_IGUAL E PUNT_COMA

TIPO -> "SHORT" 
TIPO -> "INT" 
TIPO -> "FLOAT" 
TIPO -> "CHAR" 
TIPO -> "VOID" 
TIPO -> "BOOL"

RETURN -> "RETURN" E PUNT_COMA

PRINT -> "PRINT" PARENT_INICIO E PARENT_FIN PUNT_COMA
PRINT -> "PRINT" E PUNT_COMA

READ -> "READ" ID PUNT_COMA
